<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="1.0" 
	xmlns="http://java.sun.com/xml/ns/persistence/orm" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd ">
	
	<entity name="CCRTB004_CANAL_ATENDIMENTO" class="br.gov.caixa.ccr.dominio.transicao.CanalAtendimentoTO">
		<named-query name="CanalAtendimentoTO.consultarCanal">
			<query>
				SELECT T
				  FROM br.gov.caixa.ccr.dominio.transicao.CanalAtendimentoTO T
				  WHERE T.id = :pID  
			</query>
		</named-query>
	</entity>
</entity-mappings>